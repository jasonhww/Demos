<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.jasonhww.customviewdemo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="org.jasonhww.customviewdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".scroolmethod.ScrollActivity"/>
        <activity android:name="org.jasonhww.customviewdemo.hcp.draw.MainHcp6Activity"/>
        <activity android:name="org.jasonhww.customviewdemo.hcp.draw.MainHcpC7Activity"/>
        <activity android:name="org.jasonhww.customviewdemo.hcp.draw.MainHcpC8Activity"/>
    </application>

</manifest>